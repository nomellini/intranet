# Table structure for table 'cpu'
#
DROP TABLE IF EXISTS cpu;
CREATE TABLE cpu (
  controle smallint(5) unsigned DEFAULT '0' NOT NULL,
  usuario varchar(100),
  serie varchar(50),
  processador varchar(50),
  placamae varchar(50),
  ram varchar(10),
  dvideo varchar(50),
  drede varchar(50),
  dsom varchar(50),
  dmodem varchar(50),
  dusb varchar(50),
  zip tinyint(1),
  dr144 tinyint(1),
  cdrom tinyint(1),
  obs longtext,
  hd1 text,
  hd2 text,
  PRIMARY KEY (controle)
);

