DROP TABLE IF EXISTS perifericos;
CREATE TABLE perifericos (
  controle smallint(6),
  marcamodelo varchar(100),
  serie varchar(50),
  local varchar(50),
  id_tipo smallint(6),
  obs longtext,
  controle_p int(11),
  id_periferico int(11) DEFAULT '0' NOT NULL auto_increment,
  PRIMARY KEY (id_periferico)
);

